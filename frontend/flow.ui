   <string notr="true">#Flow{
	background-color: #191D24;
}

QLabel{
	color: #ffffff;
}

QLineEdit{
	background-color: #2D3340;
	border: none;
	border-radius: 7px;
	padding-left: 5px;
}

QPushButton{
	background-color: #2D3340;
	border: none;
	border-radius: 7px;
	padding: 5px;
}

QPushButton:hover{
	background-color: #292e38;
}

QComboBox{
	background-color: #2D3340;
	border: none;
	border-radius: 7px;
	padding-left: 5px;
}

QComboBox:hover{
	background-color: #292e38;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    border-left-width: 1px;
    border-left-color: #1F232C;
    border-left-style: solid;
    border-top-right-radius: 3px;
	border-bottom-right-radius: 3px;
}

QComboBox::down-arrow {
	image: url(&quot;:/images/icons/arrow-down.svg&quot;);
	width: 10px;
	height: 10px;
}

#Flow::separator {
	width: 0.5px;
	height: 0px;
}

#DockTopWidget{
	background-color: #1F232C;
}

#DockTopWidget QPushButton, #clock{
	background-color: #FFA60A;
	border-radius: 9px;
	border: 1px solid #191D24;
	color: #000;
}

#DockTopWidget QPushButton:hover{
	background-color: #ca8102;
}

#clock{
	padding: 0px 7px 2px 7px;
	font-size: 23px;
	font-weight: bold;
}

#date{
	font-size: 14px;
}


/* Slider */

QSlider::groove:horizontal {
border: 0px solid #191d24;
background: #191D24;
height: 8px;
border-radius: 2px;
}

QSlider::sub-page:horizontal {
background: #ffa60a;
border: 0px solid #191d24;
height: 8px;
border-radius: 2px;
}

QSlider::add-page:horizontal {
background: #191D24;
border: 0px solid #191d24;
height: 8px;
border-radius: 2px;
}

QSlider::handle:horizontal {
background: transparent;
border: 0px solid transparent;
width: 0px;
}

QSlider::handle:horizontal:hover {
background: rgba(31, 35, 44, 255);
border: 1px solid #1f232c;
border-radius: 4px;
}

QSlider::sub-page:horizontal:disabled {
background: #bbb;
border-color: #999;
}

QSlider::add-page:horizontal:disabled {
background: #eee;
border-color: #999;
}

QSlider::handle:horizontal:disabled {
background: #eee;
border: 1px solid #191d24;
border-radius: 6px;
}


/* QTabWidget */

QTabWidget::tab-bar {
    border: none;
    border-radius: 0px;
}

QTabBar::tab {
    background: #2D3340;
    border: none;
    padding:  5px 12px;
    font-weight:  bold;
    color: #fff;
    border-radius: 7px;
    margin-bottom: 7px;
	margin-top: 7px;
	margin-left: 7px;
}

QTabBar::tab:hover {
    background: #282D39;
}

QTabBar::tab:selected {
    background: #FFA60A;
    font-weight: bold;
    color: #000;
}


/* Doc Catalog */

#DocCatalog, #DocCatalogWidget{
	background-color: #191D24;
}

#DocCatalog QScrollArea{
	background-color: #1F232C;
	border: none;
	border-radius: 10px;
}

#FilesListContent, #FoldersListContent{
	background-color: #1F232C;
	border-radius: 10px;
}

#DocCatalog QLabel{
	font-weight: bold;
	color: #fff;
}

FileWidget{
	background-color: #2D3340;
	border-radius: 7px;
}

FileWidget:hover{
	background-color: #292e38;
}


/* Central Widget */

#centralwidget{
	background-color: #2D3340;
}

#PlayerControl{
	background-color: #1F232C;
}

#PlayerControl QPushButton{
	background-color: transparent;
	border: none;
	border-radius: 17px;
	padding: 0px;
}

#PlayerControl QPushButton#btnPlay, #PlayerControl QPushButton#btnPause{
	border-radius: 27px;
}

#PlayerControl QPushButton:hover{
	background-color: #0c101a;
}

#PlaylistScroll, #PlaylistScroll #PlaylistContent{
	background: transparent;
	border: none;
}

ProgramBlock{
	background: #1F232C;
	border-radius: 12px;
}

ProgramBlock #BlockContent{
	background: #272C37;
	border: none;
	border-radius: 10px;
}

ProgramBlock PlaylistItem{
	background: #0066A5;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1,
                                stop:0 #418DB4, stop:1 #0066A5);
	border-radius: 10px;
}

